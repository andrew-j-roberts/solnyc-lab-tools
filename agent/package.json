{
  "name": "orchestrator",
  "version": "1.0.0",
  "description": "coordinates running sdkperf processes on perf hosts",
  "main": "index.js",
  "author": "Andrew Roberts",
  "license": "MIT",
  "scripts": {
    "build": "babel src -d dist --presets @babel/preset-env",
    "dev": "nodemon --watch src --exec 'yarn run build && yarn run start'",
    "start": "node dist/index.js",
    "test": "jest test/agent.test.js",
    "test-watch": "nodemon --watch src --exec yarn run test"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "immer": "^8.0.1",
    "mqtt": "^3.0.0",
    "regenerator-runtime": "^0.13.3",
    "shelljs": "^0.8.3",
    "xstate": "^4.7.6"
  }
}
